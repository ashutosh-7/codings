#include <iostream>
#include<string>
#include<algorithm>
#include<cmath>
#include <boost/algorithm/string.hpp>
using namespace std;

int main() {
	
	int n,m,x;
	long long sum;
	string s;
	string q;
	sum=0;
	cin>>s;
	string give;
  transform(s.begin(), s.end(), s.begin(), ::toupper); 
	give="@ZYXWVUTSRQPONMLKJIHGFEDCBA";
		n=s.size();
		x=n-1;
		for(int i=0;i<n;i++)
		{
		q=s[i];
			size_t found = give.find(q);
			sum=sum+found*(pow(26,x));
			
		
			x--;
		}
		cout<<sum;
		
	
	return 0;
}
