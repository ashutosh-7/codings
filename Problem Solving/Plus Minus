𝐏𝐫𝐨𝐛𝐥𝐞𝐦----

𝐈𝐧 𝐡𝐚𝐜𝐤𝐞𝐫𝐫𝐚𝐧𝐤 𝐩𝐫𝐚𝐜𝐭𝐢𝐜𝐞 𝐬𝐞𝐬𝐬𝐢𝐨𝐧 𝐨𝐟 Problem Solving.




𝐒𝐨𝐥𝐮𝐭𝐢𝐨𝐧𝐬----




#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
int n;
vector<int>v1;
int i,j,k;float p=0,z=0,n1=0;
cin>>n;
for(i=0;i<n;i++)
{
cin>>j;
v1.push_back(j);
if(j>0)
{
    p++;
}
else if(j==0)
{
    z++;
}
else{
    n1++;
}
}
cout<<fixed<<setprecision(6)<<p/n<<endl;
cout<<fixed<<setprecision(6)<<n1/n<<endl;
cout<<fixed<<setprecision(6)<<z/n<<endl;
    return 0;
}
